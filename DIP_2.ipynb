    "import cv2\n",
    "\n",
    "imgs = cv2.imread('salt_noise_bald_40%.jpg')\n",
    "# imgd = cv2.blur(imgs,(3,3)) #3*3的視窗濾雜訊，平均值濾波器\n",
    "imgd = cv2.medianBlur(imgs,(7)) #中值濾波器\n",
    "\n",
    "# cv2.imshow('noise',imgs)\n",
    "# cv2.imshow('result',imgd)\n",
    "\n",
    "cv2.imwrite('salt_7X7.jpg' , imgd)\n",
    "cv2.waitKey(0)"
